<h1>users#edit</h1>

<h2>プロフィール編集</h2>
<%= form_with model: @user, method: :patch do |f| %>
<div>
<%= flash[:update] %>
</div>

  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>

  <label for="inputIntroduction">Introduction</label>
  <%= f.text_field :introduction, autofocus: true, id:"introduction", placeholder:"自己紹介文"%>

  <%= f.submit "変更を保存" %>
<% end %>
<%= link_to 'Back', :back %>